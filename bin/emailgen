#!/usr/bin/env python3

from string import ascii_lowercase, digits
from random import choice

def gen(n):
    a = "".join(choice(ascii_lowercase + digits) for i in range(n))
    return a

def main():
    print(gen(2) + "." + gen(3) + "." + gen(3) + "  " + "@gmail.com")

if __name__ == "__main__":
    main()
